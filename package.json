{
    "name": "seyfert-template",
    "version": "1.0.0",
    "description": "A simple template for Seyfert projects.",
    "main": "./dist/index.js",
    "author": "Ganyu Studios",
    "type": "module",
    "license": "MIT",
    "packageManager": "pnpm@10.17.0+sha512.fce8a3dd29a4ed2ec566fb53efbb04d8c44a0f05bc6f24a73046910fb9c3ce7afa35a0980500668fa3573345bd644644fa98338fa168235c80f4aa17aa17fbef",
    "scripts": {
        "build": "tsc",
        "test": "tsx ./src/index.ts --debug --dev",
        "dev": "tsx ./src/index.ts --dev",
        "start": "node ./dist/index.js",
        "lint": "biome lint --write .",
        "format": "biome check --write .",
        "typecheck": "tsc --noEmit",
        "prepare": "husky"
    },
    "contributors": [
        {
            "url": "https://github.com/EvilG-MC",
            "name": "JustEvil"
        }
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Ganyu-Studios/seyfert-template.git"
    },
    "lint-staged": {
        "{*.ts,*.json}": [
            "pnpm format"
        ]
    },
    "keywords": [
        "seyfert",
        "template",
        "typescript",
        "node",
        "discord",
        "bot",
        "framework"
    ],
    "dependencies": {
        "dotenv": "^17.2.2",
        "seyfert": "github:tiramisulabs/seyfert",
        "yunaforseyfert": "^1.1.3"
    },
    "devDependencies": {
        "@biomejs/biome": "^2.2.4",
        "@types/node": "^24.5.2",
        "husky": "^9.1.7",
        "lint-staged": "^16.1.6",
        "tsx": "^4.20.5",
        "typescript": "^5.9.2"
    },
    "pnpm": {
        "onlyBuiltDependencies": [
            "esbuild"
        ]
    }
}
